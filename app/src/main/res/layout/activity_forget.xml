<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#03A9F4"
    android:orientation="vertical"
    tools:context=".SignUpProcess.LoginActivity">


    <androidx.cardview.widget.CardView
        android:id="@+id/cardView6"
        android:layout_width="409dp"
        android:layout_height="181dp"
        card_view:cardBackgroundColor="#00BFFF"
        card_view:cardCornerRadius="10dp"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="409dp"
            android:layout_height="181dp"
            android:background="@drawable/rectangle2">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="198dp"
                android:layout_height="147dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="106dp"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:srcCompat="@drawable/ic_baseline_bubble_chart_24" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView7"
        android:layout_width="307dp"
        android:layout_height="295dp"
        android:layout_marginTop="-20dp"
        android:layout_marginEnd="56dp"
        android:backgroundTint="#EFECEC"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintTop_toBottomOf="@+id/cardView6">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="295dp"
            android:backgroundTint="#AEA9A9">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="23dp"
                android:background="@drawable/rectangle2"
                android:gravity="center"
                android:hint="Email"
                android:textColor="#EDF2F6"
                android:textColorHint="#ECE2E2"
                android:textSize="34sp"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="TextContrastCheck" />

            <EditText
                android:id="@+id/ed1"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="23dp"
                android:ems="10"
                android:gravity="center"
                android:hint="........"
                android:inputType="textPersonName"
                android:textSize="20sp"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textView10" />

            <androidx.cardview.widget.CardView
                android:layout_width="254dp"
                android:layout_height="65dp"
                android:layout_marginTop="19dp"
                android:layout_marginEnd="26dp"
                android:backgroundTint="#00BFFF"
                card_view:cardCornerRadius="40dp"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/ed1">

                <TextView
                    android:id="@+id/btreset"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/rectangle2"
                    android:gravity="center"
                    android:text="Reset Password"
                    android:textColor="#E3DCDC"
                    android:textSize="20sp" />
            </androidx.cardview.widget.CardView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <ProgressBar
        android:id="@+id/pb1"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="195dp"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintTop_toBottomOf="@+id/cardView7" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="80dp"
        android:layout_height="82dp"
        android:layout_marginTop="107dp"
        android:layout_marginEnd="40dp"
        card_view:layout_constraintEnd_toStartOf="@+id/imageView11"
        card_view:layout_constraintTop_toBottomOf="@+id/cardView7"
        card_view:srcCompat="@drawable/ic_baseline_house_24" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="70dp"
        android:layout_height="75dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="47dp"
        card_view:layout_constraintEnd_toStartOf="@+id/imageView12"
        card_view:layout_constraintTop_toBottomOf="@+id/pb1"
        card_view:srcCompat="@drawable/ic_baseline_grass_24" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="70dp"
        android:layout_height="75dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="56dp"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintTop_toBottomOf="@+id/cardView7"
        card_view:srcCompat="@drawable/ic_baseline_golf_course_24" />

</androidx.constraintlayout.widget.ConstraintLayout>